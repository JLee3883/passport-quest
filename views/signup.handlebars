            <section class="card" style="width: 600px; height: auto;">

                <h3 id="title-">Sign Up</h3>
                <form>
                    <div class="form-group">
                        <p>
                            <label for="username"> UserName:*</label><input type="text" id="userName" name="userName" style="color: #000000 !important"></input>
                        </p>
                        <p>
                            <label for="email">Email:*</label><input type="text" id="email" name="email" style="color: #000000 !important"></input>
