<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Passport Quest</title>
    <link rel="stylesheet" href="./css/addprofile/reset.css" />
    <link rel="stylesheet" href="./css/addprofile/style.css" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link href="https://fonts.googleapis.com/css2?family=Comic+Neue&family=Fuzzy+Bubbles&family=Rock+Salt&display=swap" rel="stylesheet" />
    <link rel="stylesheet" href="//code.jquery.com/ui/1.13.0/themes/base/jquery-ui.css">
    <link rel="stylesheet" href="/resources/demos/style.css">
    <script src="https://code.jquery.com/jquery-3.6.0.js"></script>
    <script src="https://code.jquery.com/ui/1.13.0/jquery-ui.js"></script>
</head>

<body>
    <header>
        <h1 id="title-name">Passport Quest</h1>
    </header>
    <nav>
        <div class="navbar">
            <a class="jump" href="#about-me">Log Out</a>
            <a class="jump" href="#profile">Profile</a>
            <a class="jump" href="#home">Home</a>
            <a class="jump" href="#software">About Us</a>
            <a class="jump" href="#graphics">Help</a>
        </div>
    </nav>
    <main>
        <div class="container" style="font-family: Fuzzy Bubbles, cursive; color: Black">
            <section class="card" style="width: 600px; height: auto;">
                <h3 id="title-">Add Trip</h3>
                <form>
                    <p class="form-group">
                        <p><label for="title"> Title:*</label><input type="text" id="userName" name="userName" style="color: #000000 !important"></input>
                        </p>
                        <p><label for="country">Country:</label><input type="text" id="country" name="country" style="color: #000000 !important"></input>
                        </p>
                        <p><label for="city"> City:* </label><input type="text" id="city" name="city" style="color: #000000 !important"></input>
                        </p>
                        <p><label for="StartDate"> Start Date:* </label><input type="text" id="datepicker" style="color: #000000 !important"></p>
                        <p><label for="EndDate"> End Date:* </label><input type="text" id="datepicker" style="color: #000000 !important"></p>
                        <p><label for="lastName"> Bodget: </label><input type="text" id="bodget" name="bodget" style="color: #000000 !important"></input>
                        </p>
                        <p><label for="nationality"> Accomodation:* </label><input type="text" id="accomodation" name="accomodation" style="color: #000000 !important"></input>
                        </p>
                        <p><label for="currentLocation"> Transportation:* </label><input type="text" id="transportation" name="transportation" style="color: #000000 !important"></input>
                        </p>
                        <p><label for="homeCountry"> Emergency Name:* </label><input type="text" id="emergencyName" name="emergencyName" style="color: #000000 !important"></input>
                        </p>
                        <p><label for="homeCountry"> Emergency Contact:* </label><input type="text" id="emergencyContact" name="emergencyContact" style="color: #000000 !important"></input>
                        </p>
                        <p><label for="homeCountry">Note: </label><input type="text" id="note" name="note" style="color: #000000 !important"></input>
                        </p>
                </form>
                <ul class="actions">
                    <input href="#profile-page" type="submit" value="submit" class="button primary">
                </ul>
        </div>
    </main>
    <script src="./JS/JavaScript.js"></script>


</body>

</html>