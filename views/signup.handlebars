                        <p>
                            <label for="firstName"> First Name:* </label><input type="text" id="firstName" name="firstName" style="color: #000000 !important"></input>
                        </p>
                        <p>
                            <label for="middleName"> Middle Name: </label><input type="text" id="middleName" name="middleName" style="color: #000000 !important"></input>
                        </p>
                        <p>
                            <label for="lastName"> Last Name:* </label><input type="text" id="lastName" name="lastName" style="color: #000000 !important"></input>
                        </p>
                        <p>
                            <label for="birthday"> Birthday:* </label><input type="text" id="datepicker" style="color: #000000 !important"></p>
                        <p>
                            <label for="nationality"> Nationality:* </label><input type="text" id="nationality" name="nationality" style="color: #000000 !important"></input>
                        </p>
                        <p>
                            <label for="currentLocation"> Current Location: </label><input type="text" id="currentLocation" name="currentLocation" style="color: #000000 !important"></input>
                        </p>
                        <p>
